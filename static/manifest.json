{
    "manifest_version": 3,
    "name":"Ai meeting assistant",
    "version":"1.0",
    "description":"This is my first extension",
    "action":{
        "default_popup":"index.html",
        "default_title":"ai meeting assistant!"
    },
    "permissions":[
        "activeTab",
        "storage",
        "scripting"
    ],
    "host_permissions":[
        "https://meet.google.com/*"
    ]
}